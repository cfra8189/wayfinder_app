{
  "builds": [
    {
      "src": "wayfinder_app-v2/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/public" }
    }
  ],
  "routes": [
    { "src": "^/assets/(.*)$", "headers": { "cache-control": "public, max-age=31536000, immutable" }, "dest": "/src/assets/$1" },
    { "src": "^(?!/api|/assets)(.*)$", "dest": "/index.html" }
  ]
}
